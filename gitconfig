[color]
  ui = auto
[color "branch"]
  current = yellow reverse
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[core]
  whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
  quotepath = false
  excludesfile = ~/.gitexcludes
  ff = only
  fileMode = false
[alias]
  st = status
  cm = commit
  br = branch
  co = checkout
  df = diff
  mg = merge --no-ff
  lg = log -p
  l1 = log --graph --decorate --oneline --stat -1
  l2 = log --graph --decorate --oneline --stat -2
  l3 = log --graph --decorate --oneline --stat -3
  l4 = log --graph --decorate --oneline --stat -4
  l5 = log --graph --decorate --oneline --stat -5
  l0 = log --graph --decorate --oneline --stat -10
  lol = log --graph --decorate --oneline
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
  tags = tag --list
  ignore = update-index --assume-unchanged
  unignore = update-index --no-assume-unchanged
  ignored = !git ls-files -v | grep "^[[:lower:]]"
[merge]
  tool = vimdiff
[mergetool]
  keepBackup = false
[rerere]
  enabled = 0
[branch]
  autosetupmerge = true

[push]
  default = matching

